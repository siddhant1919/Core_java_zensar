package com.collect;

import java.util.*;
public class DemoCollection 
{

	public static void main(String[] args) 
	{
		ArrayList a1 = new ArrayList();
		a1.add(3000);
		a1.add("java");
		a1.add(new Float(20.0f));
		a1.add(new Float(20.0f));
		a1.add(new Double(200.4));
		
		HashSet s1 = new HashSet();
		s1.add("Java");
		s1.add("Spring");
		s1.add("Hibernationg");
		s1.add("Angular");
		
		System.out.println(s1);
	}

}
