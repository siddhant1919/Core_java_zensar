package com.collect;

import java.util.*;
public class DemoCollection 
{

	public static void main(String[] args) 
	{
		ArrayList a1 = new ArrayList();
		a1.add(3000);
		a1.add("java");
		a1.add(new Float(20.0f));
		a1.add(new Float(20.0f));
		a1.add(new Double(200.4));
		
		System.out.println(a1);
	}

}
